
   <%- include('partials/header') %>

   <% users.forEach(user => { %>
    <div class="flex flex-wrap leading-normal text-5xl md:text-3xl lg:max-w-lg bg-white rounded shadow-lg mx-auto my-8">
      <div class="photo-column">
        <img class="min-w-full w-xl lg:w-md lg:h-sm object-cover rounded" title="" src="">
      </div>
      <div class="px-6 py-4">
       
          <div class="font-bold mb-2"> <%= user.fullname %> <span class="inline-block bg-gray-200 rounded-full px-3 py-1 text-sm font-semibold text-gray-700 mr-2">SS3A</span></div>
    
        <p class="mb-3"><span class="text-gray-600">Lives in 🏡</span>: <%= user.residence %> </p>
        
        
      
    
        <p class="mb-3"><span class="text-gray-600">Occupation 👷‍♀: <%= user.occupation%></span> </p>
        
       
        <p class="mb-3">
          <span class="text-gray-600">Date of birth 📅: <%= user.month %> <%= user.year %></span> 
          <p class="text-xl mt-2"></p>
        </p>
      </div>
      
      </div>
    </div>
         
    <% }) %>
  
      
   
  <script src="src/index.js"></script>
</body>
</html>